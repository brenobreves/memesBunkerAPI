generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  name          String
  email         String    @unique
  password_hash String
  id            String    @id @default(uuid()) @db.Uuid
  Sessions      Sessions[]
}

model Sessions {
  id            String    @id @default(uuid()) @db.Uuid
  user_id       String    @db.Uuid
  user          User      @relation(fields: [user_id], references: [id])
  createdAt     DateTime  @default(now())
}
